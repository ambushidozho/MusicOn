<div class="player">
    <audio preload="meta" src="{{this.Content}}"></audio>
    <div class="progress-bar">
        <div class="progress-bar__progress"></div>
    </div>
    <div class="player__container">
        <div class="prev-btn"><img class="player-btns" data-section="prevBtn" src="/static/img/SwitchBack.svg" alt=""></div>
        {{#if Playing}}
        <div class="play-btn"><img class="player-btns"  data-section="playBtn" src="/static/img/Pause.svg" alt=""></div>
        {{else}}
        <div class="play-btn"><img class="player-btns"  data-section="playBtn" src="/static/img/Play.svg" alt=""></div>
        {{/if}}
        {{#each PlayerComponentConfig}}
             <div class="{{this.key}}"><img class="player-btns" data-section="{{this.key}}" src="{{this.href}}" alt=""></div>
        {{/each}}

        <div><img class="avatar" src="{{port}}{{song.Preview}}" alt="asd"></div>
        <div>
            <div class="title medium-text">{{song.Name}}</div>
            <div class="artist small-text">{{song.ArtistName}}</div>
        </div>
        {{#if Auth}}
            {{#if song.isLiked}}
            <div class="like-btn"><img class="player-btns" data-section="likeBtn" src="/static/img/LikePressed.svg" alt=""></div>
            {{else}}
            <div class="like-btn"><img class="player-btns" data-section="likeBtn" src="/static/img/Like.svg" alt=""></div>
            {{/if}}
        {{/if}}
        
        <div class="volume-btn"><img class="player-btns"  data-section="volumeBtn"src="/static/img/SoundOn.svg" alt=""></div>
        <input class="volume-bar__volume-btn" type='range' min="0" max="100" value="0" step="1" oninput="rangevalue.value=value">
        {{!-- <div class="volume-bar">
            
            <div class="volume-bar__volume"></div>
        </div> --}}
    </div>
</div>